{
  "title": "下拉菜单",
  "namespace": "mybricks.normal-pc.dropdown",
  "version": "1.0.18",
  "description": "下拉菜单",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "inputs": [],
  "outputs": [
    {
      "id": "onChange",
      "title": "点击",
      "schema": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "link": {
            "type": "string"
          },
          "key": {
            "type": "string"
          }
        }
      }
    }
  ],
  "slots": [
    {
      "id": "carrier",
      "title": "添加组件"
    },
    {
      "id": "item",
      "capacity": 1,
      "title": "选项",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项",
          "desc": "当前项展示内容",
          "schema": {
            "type": "any"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "desc": "当前项展示的序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    }
  ]
}