{
  "title": "级联选择",
  "namespace": "mybricks.normal-pc.cascader",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.1.8",
  "description": "级联选择",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_fw2jtc.d560e08aa8edba85.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置级联选择的值，并触发值变化事件输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "any"
        }
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置级联选择的初始值，并触发初始化事件输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "any"
        }
      },
      "rels": [
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发级联选择的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取级联选择的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空级联选择的值",
      "schema": {
        "type": "any"
      },
      "rels": [
        "resetValueDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项编辑/只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "setOptions",
      "title": "输入数据源",
      "desc": "设置级联选择的下拉选项列表",
      "schema": {
        "title": "输入数据源数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "value": {
              "title": "值",
              "type": "any"
            },
            "label": {
              "title": "名称",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "type": "boolean"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {}
              }
            }
          }
        }
      },
      "rels": [
        "setOptionsDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "schema": {
        "title": "当前值",
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "any"
        }
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "any"
        }
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setOptionsDone",
      "title": "设置数据源完成",
      "schema": {
        "title": "输入数据源数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "value": {
              "title": "值",
              "type": "any"
            },
            "label": {
              "title": "名称",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "type": "boolean"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {}
              }
            }
          }
        }
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}