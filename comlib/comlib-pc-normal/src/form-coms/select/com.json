{
  "title": "下拉框",
  "namespace": "mybricks.normal-pc.select",
  "version": "1.1.14",
  "description": "下拉框",
  "author": "ZhuLin",
  "author_name": "朱琳",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_0nxwk5.ce543f08d939685b.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "ai": "./ai.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置下拉框的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setValueDone"
      ]
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置下拉框的初始值，并触发值初始化事件输出",
      "schema": {
        "type": "string"
      },
      "rels":[
        "setInitialValueDone"
      ]
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发下拉框的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取下拉框的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空下拉框的值",
      "schema": {
        "type": "any"
      },
      "rels":[
        "resetValueDone"
      ]
    },
    {
      "id": "setOptions",
      "title": "设置数据源",
      "desc": "设置下拉框的下拉选项列表",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "type": "string"
            },
            "value": {
              "title": "值",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "type": "boolean"
            },
            "checked": {
              "title": "选中",
              "type": "boolean"
            }
          }
        }
      },
      "rels":[
        "setOptionsDone"
      ]
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setDisabledDone"
      ]
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      },
      "rels": [
        "setEnabledDone"
      ]
    },
    {
      "id": "isEnable",
      "title": "启用/禁用",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEnableDone"
      ]
    },
    {
      "id": "isEditable",
      "title": "设置编辑/只读",
      "desc": "设置表单项只读态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "isEditableDone"
      ]
    },
    {
      "id": "setLoading",
      "title": "设置加载中状态",
      "schema": {
        "type": "boolean"
      },
      "rels": [
        "setLoadingDone"
      ]
    },
    {
      "id": "setColor",
      "title": "设置字体颜色",
      "desc": "设置下拉框里字体的颜色",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setColorDone"
      ]
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验结果",
      "desc": "配合自定义校验使用，设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setValidateInfoDone"
      ]
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onBlur",
      "title": "失去焦点",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValueDone",
      "title": "设置值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValueDone",
      "title": "设置初始值完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "resetValueDone",
      "title": "重置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setOptionsDone",
      "title": "设置数据源完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "title": "标签",
              "type": "string"
            },
            "value": {
              "title": "值",
              "type": "string"
            },
            "disabled": {
              "title": "禁用",
              "type": "boolean"
            },
            "checked": {
              "title": "选中",
              "type": "boolean"
            }
          }
        }
      }
    },
    {
      "id": "setDisabledDone",
      "title": "禁用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabledDone",
      "title": "启用完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "isEnableDone",
      "title": "启用/禁用完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "isEditableDone",
      "title": "设置编辑/只读完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setLoadingDone",
      "title": "设置加载中状态完成",
      "schema": {
        "type": "boolean"
      }
    },
    {
      "id": "setColorDone",
      "title": "设置字体颜色完成",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setValidateInfoDone",
      "title": "设置校验状态完成",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}